#include <iostream>
#include <string>
using namespace std;
int main()
{	int num[3][2] = {3,1,5,4,6,8};
	cout<<num[0][0]<<"   "<<num[0][1]<<endl;
	cout<<num[1][0]<<"   "<<num[1][1]<<endl;
	cout<<num[2][0]<<"   "<<num[2][1]<<endl;
	cout<<"get value from array 2D with for"<<endl;
	for(int i=0;i<3;i++)
	{	for(int j=0;j<2;j++)
		{	cout<<num[i][j]<<"   ";
		}
		cout<<endl;
	}
	system ("pause");
	return 0;
}

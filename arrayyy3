#include <iostream>
#include <string>
using namespace std;
int main()
{
	int num;
	//string name[10];
	cout<<"Enter Number of Student : ";
	cin>>num;
	int *score = new int[num];
	string *name = new string[num];
	for (int i=0;i<num;i++)
	{	cout<<"Enter Name : ";
		cin>>name[i];
		cout<<"Enter score : ";
		cin>>score[i];
	}
	system ("pause");
	return 0;
}
